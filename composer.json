{
  "name": "IASC/Extranet",
  "description": "Extranet drupal codebase for IASC.",
  "config": {
    "bin-dir": "bin"
  },
  "require-dev": {
    "drupal/drupal-extension": "v0.1.5",
    "behat/behat": ">=2.5@stable",
    "behat/gherkin": ">=2.3@stable",
    "behat/mink": ">=1.5@stable",
    "behat/mink-browserkit-driver": "*",
    "behat/mink-extension": "*",
    "behat/mink-goutte-driver": "*",
    "behat/mink-sahi-driver": "*",
    "behat/mink-selenium2-driver": "*",
    "behat/mink-zombie-driver": "*",
    "phpmd/phpmd": "1.4.*",
    "pdepend/pdepend": "1.1.*",
    "squizlabs/php_codesniffer": "1.*",
    "drupal/coder": "7.2.2"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "drupal/coder",
        "version": "7.2.2",
        "dist": {
          "url": "http://ftp.drupal.org/files/projects/coder-7.x-2.2.zip",
          "type": "zip"
        },
        "source": {
          "url": "http://git.drupal.org/project/coder.git",
          "type": "git",
          "reference": "tags/7.x-2.2"
        }
      }
    }
  ]
}