include_once 'iasc_announcement.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function iasc_announcement_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/content_types';
  }
}
